#!/bin/sh
if test "x${NRNHOME}" = "x" ; then
    NRNHOME="/Users/kumbhar/workarena/repos/bbp/nn/build/cmake_install"
fi
if test "x${NRNBIN}" = "x" ; then
    NRNBIN="/Users/kumbhar/workarena/repos/bbp/nn/build/cmake_install/bin"
fi
if test "" = "yes" ; then
    NRNIV="${NRNBIN}/nrniv.app/Contents/MacOS/nrniv"
else
    NRNIV="${NRNBIN}/nrniv"
fi
"${NRNIV}" -dll "/Users/kumbhar/workarena/repos/bbp/nn/build/cmake_install/share/nrn/demo/release/x86_64/.libs/libnrnmech.so" "$@"

#!/Library/Frameworks/Python.framework/Versions/3.6/bin/python3.6
# EASY-INSTALL-ENTRY-SCRIPT: 'microstructpy==1.2.1','console_scripts','microstructpy'
__requires__ = 'microstructpy==1.2.1'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('microstructpy==1.2.1', 'console_scripts', 'microstructpy')()
    )

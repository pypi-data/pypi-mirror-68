{
  "version": 1,
  "disable_existing_loggers": false,
  "formatters": {
    "long": {
      "format": "%(levelname)s - %(asctime)s - %(module)s - %(message)s"
    },
    "short": {
      "format": "%(levelname)s:%(module)s:%(message)s"
    }
  },
  "handlers": {
    "file": {
      "level": "INFO",
      "formatter": "long",
      "class": "logging.handlers.RotatingFileHandler",
      "filename": "FIND_HOME.log",
      "maxBytes": 10485760,
      "backupCount": 20,
      "encoding": "utf8"
    },
    "console": {
      "level": "INFO",
      "formatter": "long",
      "class": "logging.StreamHandler",
      "stream": "ext://sys.stdout"
    }
  },
  "loggers": {
    "prod_logger": {
      "level": "INFO",
      "handlers": [
        "file"
      ]
    }
  },
  "root": {
    "level": "INFO",
    "handlers": [
      "console", "file"
    ]
  }
}

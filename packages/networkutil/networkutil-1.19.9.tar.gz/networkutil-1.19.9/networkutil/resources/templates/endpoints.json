{

  "environments": [
    "production",
    "lab"
  ],

  "apis": {
    "example_api": {
      "production": {
        "methods": {
          "m1": "1.0"
        },
        "key": "12345"
      },
      "lab": {
        "methods": {
          "m1": "2.0/some_endpoint",
          "m2": "2.0/some_other_endpoint"
        },
        "key": "67890"
      }
    },
    "example_api_2": {
      "production": {
        "methods": {
          "m1": "1.2"
        },
        "key": "12345",
        "parameters": {
          "p1": "some_param"
        }
      },
      "lab": {
        "methods": {
          "m1": "2.2/some_endpoint",
          "m2": "2.2/some_other_endpoint"
        },
        "key": "67890"
      }
    }
  },

  "endpoints": {
    "server1.example.com": {
      "port": 80,
      "environment": "production",
      "apis": [
        "example_api"
      ]
    },
    "server1.lab.example.com": {
      "port": 80,
      "environment": "lab",
      "apis": [
        "example_api",
        "example_api_2"
      ]
    }
  }

}
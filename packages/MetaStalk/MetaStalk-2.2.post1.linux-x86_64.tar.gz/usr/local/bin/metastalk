#!python
# EASY-INSTALL-ENTRY-SCRIPT: 'MetaStalk==2.2.post1','console_scripts','metastalk'
__requires__ = 'MetaStalk==2.2.post1'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('MetaStalk==2.2.post1', 'console_scripts', 'metastalk')()
    )
